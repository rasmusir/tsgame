    "ws": "^1.1.1"
  },
  "devDependencies": {
    "@types/ejs": "^2.3.33",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.4",
    "@types/three": "^0.83.1",
    "@types/ws": "0.0.38",
    "browserify": "^14.0.0",
    "npm-run-all": "^4.0.1",
    "rand-token": "^0.3.0",
    "three": "^0.84.0",
    "tsify": "^3.0.0",
    "typescript": "^2.1.5"
